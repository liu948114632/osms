<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.itecheasy.test.hibernateG.BaseProductCostPriceCurrencyEntity"
           table="base_product_cost_price_currency" schema="dbo" catalog="Develop_OSMS">
        <id name="id">
            <column name="id" sql-type="int"/>
        </id>
        <property name="currency">
            <column name="currency" sql-type="int" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>