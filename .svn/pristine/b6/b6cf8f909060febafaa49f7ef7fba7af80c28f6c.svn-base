package com.itecheasy.core.fba;

import java.util.List;

/**
 * @author taozihao
 * @date 2018年6月8日 上午10:48:53
 * @description 准备提交给亚马逊的补货订单展示
 */
public class FbaPreInboundOrderVO {

	private String planName;
	private String shipmentId;
	private String shipmentName;
	private FbaToAddress shipToAddressVO;
	private double totalWeight;
	private int totalAmount;
	private List<FbaPreInboundItemVO> items;

	public String getPlanName() {
		return planName;
	}

	public void setPlanName(String planName) {
		this.planName = planName;
	}
	
	public String getShipmentId() {
		return shipmentId;
	}

	public void setShipmentId(String shipmentId) {
		this.shipmentId = shipmentId;
	}

	public String getShipmentName() {
		return shipmentName;
	}

	public void setShipmentName(String shipmentName) {
		this.shipmentName = shipmentName;
	}

	public FbaToAddress getShipToAddressVO() {
		return shipToAddressVO;
	}

	public void setShipToAddressVO(FbaToAddress shipToAddressVO) {
		this.shipToAddressVO = shipToAddressVO;
	}

	public double getTotalWeight() {
		return totalWeight;
	}

	public void setTotalWeight(double totalWeight) {
		this.totalWeight = totalWeight;
	}

	public int getTotalAmount() {
		return totalAmount;
	}

	public void setTotalAmount(int totalAmount) {
		this.totalAmount = totalAmount;
	}

	public List<FbaPreInboundItemVO> getItems() {
		return items;
	}

	public void setItems(List<FbaPreInboundItemVO> items) {
		this.items = items;
	}

}
