package com.itecheasy.core.fba;

import java.util.List;

/**
 * @author wanghw
 * @date 2016-12-5
 * @description TODO
 * @version 1.2.2
 */
public class ReplenishmentOrderDetail {
	private ReplenishmentOrder replenishmentOrder;
	private List<ReplenishmentOrderItem> replenishmentOrderItems;
	private Repository repository;
	//是否为自动建单
	private boolean isAutoGenerated;
	private boolean shippingMethodChangeable;

	public boolean isShippingMethodChangeable() {
		return shippingMethodChangeable;
	}

	public void setShippingMethodChangeable(boolean shippingMethodChangeable) {
		this.shippingMethodChangeable = shippingMethodChangeable;
	}

	public boolean isAutoGenerated() {
		return isAutoGenerated;
	}

	public void setAutoGenerated(boolean isAutoGenerated) {
		this.isAutoGenerated = isAutoGenerated;
	}

	public Repository getRepository() {
		return repository;
	}

	public void setRepository(Repository repository) {
		this.repository = repository;
	}

	public ReplenishmentOrder getReplenishmentOrder() {
		return replenishmentOrder;
	}

	public void setReplenishmentOrder(ReplenishmentOrder replenishmentOrder) {
		this.replenishmentOrder = replenishmentOrder;
	}

	public List<ReplenishmentOrderItem> getReplenishmentOrderItems() {
		return replenishmentOrderItems;
	}

	public void setReplenishmentOrderItems(List<ReplenishmentOrderItem> replenishmentOrderItems) {
		this.replenishmentOrderItems = replenishmentOrderItems;
	}

}
