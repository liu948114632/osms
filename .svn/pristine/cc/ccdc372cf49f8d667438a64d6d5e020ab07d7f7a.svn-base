package com.itecheasy.core.po;

import javax.persistence.*;

/**
 * @author wanghw
 * @date 2016-1-22
 * @description TODO
 * @version
 */
@Entity
@Table(name="FBA_shipping_method")
public class FBAShippingMethodPO {
	private int id;
	private String name;

	@Id
	@GeneratedValue
	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	@Column
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

}
